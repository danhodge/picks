#!/usr/bin/env ruby

$LOAD_PATH.unshift(File.expand_path('../../app', __FILE__))
$LOAD_PATH.unshift(File.expand_path('../../app/models', __FILE__))
$LOAD_PATH.unshift(File.expand_path('../../app/services', __FILE__))

require 'bundler/setup'
Bundler.require

require 'models'

require 'irb'
ARGV.clear
IRB.start
